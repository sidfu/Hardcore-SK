			<texPath>Things/Pawn/Humanlike/Apparel/PrestigeReconHelmet/PrestigeReconHelmet</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<thingClass>SK.Apparel_PowerHelmet</thingClass>
		<tickerType>Normal</tickerType>
		<stuffCategories>
			<li>StrongMetallic</li>
		</stuffCategories>
		<costStuffCount>30</costStuffCount>
		<costList>
			<Compaste>20</Compaste>
			<ChitinPlating>30</ChitinPlating>
			<GoldBar>8</GoldBar>
			<MagneticMaterial>2</MagneticMaterial>
			<ComponentSpacer>1</ComponentSpacer>
			<Hexcell>3</Hexcell>
		</costList>
		<statBases>
			<WorkToMake>16500</WorkToMake>
			<EnergyShieldRechargeRate>0.03</EnergyShieldRechargeRate>
			<EnergyShieldEnergyMax>0.65</EnergyShieldEnergyMax>
			<MaxHitPoints>50</MaxHitPoints>
			<Flammability>1.1</Flammability>
			<Mass>1</Mass>
			<Bulk>3</Bulk>
			<WornBulk>0</WornBulk>
			<EquipDelay>1.2</EquipDelay>
			<Insulation_Heat>5</Insulation_Heat>
			<Insulation_Cold>5</Insulation_Cold>
			<StuffEffectMultiplierInsulation_Cold>0.25</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.25</StuffEffectMultiplierInsulation_Heat>
			<StuffEffectMultiplierArmor>3.9</StuffEffectMultiplierArmor>
