			<PsychicSensitivity>0.05</PsychicSensitivity>
			<PsychicEntropyRecoveryRate>0.033</PsychicEntropyRecoveryRate>
			<ShootingAccuracyPawn>0.05</ShootingAccuracyPawn>
			<AimingDelayFactor>-0.1</AimingDelayFactor>
			<GlobalLearningFactor>0.05</GlobalLearningFactor>
			<ImmunityGainSpeed>0.15</ImmunityGainSpeed>
			<Suppressability>-0.06</Suppressability>
			<ArmorRating_Toxin>0.2</ArmorRating_Toxin>
			<ToxicSensitivity>-0.5</ToxicSensitivity>
			<SmokeSensitivity>-1</SmokeSensitivity>
			<Radiation>-0.30</Radiation>
			<GermResistance>0.70</GermResistance>
			<GermContainment>0.80</GermContainment>
		</equippedStatOffsets>
		<generateCommonality>2</generateCommonality>
		<apparel>
			<bodyPartGroups>
				<li>FullHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/PrestigeReconHelmet/PrestigeReconHelmet</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>PrestigeCombatGear</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(0.33,0.33,0.33,1)</only>
				</li>
				<li>
					<weight>10</weight>
					<only>(0.65,0.65,0.65,1)</only>
				</li>
				<li>
					<weight>6</weight>
					<min>(0.3,0.3,0.3,1)</min>
					<max>(0.5,0.5,0.5,1)</max>
				</li>
				<li>
					<weight>10</weight>
					<only>(0.9,0.9,0.9,1)</only>
				</li>
			</options>
		</colorGenerator>
		<thingSetMakerTags>
			<li>RewardStandardHighFreq</li>
		</thingSetMakerTags>
		<comps>
			<li Class="CompProperties_BiocodableApparel"/>
		</comps>
		<thingCategories>
			<li>HeavyHelmetsCat</li>
		</thingCategories>
	</ThingDef>

</Defs>