<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Bad Hygiene</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationTest">
					<xpath>Defs/ThingDef[defName='DBH_WaterBottle']</xpath>
					<success>Normal</success>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName='BarShelf']/building/fixedStorageSettings/filter/thingDefs</xpath>
					<order>Prepend</order>
					<value>
						<li>DBH_WaterBottle</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>