			<partEfficiency>1.25</partEfficiency>
		</addedPartProps>
		<stages>
			<li>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>0.15</offset>
					</li>
					<li>
						<capacity>Moving</capacity>
						<offset>0.2</offset>
					</li>
				</capMods>
			</li>	 
		</stages>
		<spawnThingOnRemoved>Exoskeleton</spawnThingOnRemoved>
