	<PawnKindDef>
		<defName>ResistanceBodyguard</defName>
		<label>T-Cyborg</label>
		<race>SkynetPrototypeTX</race>
		<combatPower>1200</combatPower>
		<chemicalAddictionChance>0</chemicalAddictionChance>
		<baseRecruitDifficulty>0.99</baseRecruitDifficulty>
		<backstoryFilters>
			<li>
				<categories>
					<li>Offworld</li>
					<li>Pirate</li>
					<li>Outlander</li>
				</categories>
			</li>
		</backstoryFilters>
		<defaultFactionType>HumanResistance</defaultFactionType>
		<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
		<maxGenerationAge>30</maxGenerationAge>
		<itemQuality>Normal</itemQuality>
		<isFighter>true</isFighter>
		<aiAvoidCover>true</aiAvoidCover>
		<gearHealthRange>
			<min>0.7</min>
			<max>1.6</max>
		</gearHealthRange>
		<apparelMoney>
			<min>10000</min>
			<max>12000</max>
		</apparelMoney>
		<apparelRequired>
			<li>Apparello_GSkin</li>
		</apparelRequired>
		<apparelTags>
			<li>SyndicateMedium</li>
			<li>SyndicateUnique</li>
		</apparelTags>
		<weaponMoney>
			<min>3300</min>
			<max>4100</max>
		</weaponMoney>
		<weaponTags>
			<li>ADR2</li>
		</weaponTags>
		<inventoryOptions>
			<skipChance>0.8</skipChance>
			<subOptionsChooseOne>
				<li>
					<thingDef>AndroidRepairParts</thingDef>
					<countRange>
						<min>1</min>
						<max>2</max>
					</countRange>
				</li>
			</subOptionsChooseOne>
		</inventoryOptions>
		<modExtensions>
			<li Class="CombatExtended.LoadoutPropertiesExtension">
				<primaryMagazineCount>
					<min>24</min>
					<max>25</max>
				</primaryMagazineCount>
				<sidearms>
					<li>
						<generateChance>0.7</generateChance>
						<sidearmMoney>
							<min>250</min>
							<max>350</max>
						</sidearmMoney>
						<weaponTags>
							<li>GrenadeDestructive</li>
						</weaponTags>
						<magazineCount>
							<min>1</min>
							<max>3</max>
						</magazineCount>
					</li>
				</sidearms>
			</li>
		</modExtensions>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<apparelAllowHeadgearChance>0.4</apparelAllowHeadgearChance>
	</PawnKindDef>

