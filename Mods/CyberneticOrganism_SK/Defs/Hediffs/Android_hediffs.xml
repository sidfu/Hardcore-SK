  <!-- ############## Zoom ############## -->
  <HediffDef ParentName="AddedBodyPartBase">
    <defName>Trunken_hediff_Zoom</defName>
    <label>Zoom</label>
    <description>An enhanced artifical eye</description>
    <labelNoun>a bionic eye</labelNoun>
    <defaultLabelColor>(0.5, 0.5, 0.9)</defaultLabelColor>
    <stages>
      <li>
        <statOffsets>
          <ShootingAccuracyPawn>2</ShootingAccuracyPawn>
          <MedicalTendQualityOffset>0.1</MedicalTendQualityOffset>
          <MedicalTendSpeed>0.15</MedicalTendSpeed>
        </statOffsets>
        <capMods>
          <li>
            <capacity>Sight</capacity>
            <offset>0.05</offset>
          </li>
        </capMods>
      </li>
    </stages>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.10</partEfficiency>
    </addedPartProps>
    <spawnThingOnRemoved>ANN_Zoom</spawnThingOnRemoved>
    <descriptionHyperlinks><ThingDef>ANN_Zoom</ThingDef></descriptionHyperlinks>
  </HediffDef>

