<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationTest">
				<xpath>/Defs/ThingDef[defName="ChJAndroidPrinter"]/modExtensions/li[@Class="Androids.PawnCrafterProperties"]/disabledRaces</xpath>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="ChJAndroidPrinter"]/modExtensions/li[@Class="Androids.PawnCrafterProperties"]/disabledRaces</xpath>
				<order>Prepend</order>
				<value>
					<li>Human</li>
					<li>Alien_Orassan</li>
					<li>Norbal</li>
					<li>Alien_Nova</li>
					<li>Alien_Dova</li>
				</value>
			</li>
		</operations>
	</Operation>

	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>NewRatkinPlus</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/ThingDef[defName="ChJAndroidPrinter"]/modExtensions/li[@Class="Androids.PawnCrafterProperties"]/disabledRaces</xpath>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="ChJAndroidPrinter"]/modExtensions/li[@Class="Androids.PawnCrafterProperties"]/disabledRaces</xpath>
					<order>Prepend</order>
					<value>
						<li>Ratkin</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	
	<Operation Class="PatchOperationFindMod">
		<success>Always</success>
		<mods>
			<li>AsariRace</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationTest">
					<xpath>/Defs/ThingDef[defName="ChJAndroidPrinter"]/modExtensions/li[@Class="Androids.PawnCrafterProperties"]/disabledRaces</xpath>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName="ChJAndroidPrinter"]/modExtensions/li[@Class="Androids.PawnCrafterProperties"]/disabledRaces</xpath>
					<order>Prepend</order>
					<value>
						<li>Alien_Asari</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>