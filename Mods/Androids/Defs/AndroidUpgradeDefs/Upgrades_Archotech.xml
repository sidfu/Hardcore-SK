<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Archotech -->
	<Androids.AndroidUpgradeGroupDef>
		<defName>Archotech</defName>
		<label>Archotech</label>
		<orderID>400</orderID>
	</Androids.AndroidUpgradeGroupDef>
	
	<Androids.AndroidUpgradeDef>
		<defName>Upgrade_TechprofAIModule</defName>
		<label>Techprof Module</label>
		<description>Installs a secondary AI which greatly increases the research capabilities of the Android.\n-- "Two brains is better than one."</description>
		<commandType>Androids.UpgradeCommand_Hediff</commandType>
		<iconTexturePath>Icons/Upgrades/processor</iconTexturePath>
		<orderID>0</orderID>
		<upgradeGroup>Archotech</upgradeGroup>
		<costList>
			<BioMicrochips>1</BioMicrochips>
			<TechprofSubpersonaCore>1</TechprofSubpersonaCore>
		</costList>
		<costsNotAffectedByBodySize>
			<li>TechprofSubpersonaCore</li>
		</costsNotAffectedByBodySize>
		<hediffToApply>ChjAndroidUpgrade_TechprofAIModule</hediffToApply>
		<extraPrintingTime>60000</extraPrintingTime>
		<requiredResearch>ChJAndroidUpgrade_Archotech</requiredResearch>
	</Androids.AndroidUpgradeDef>

	<Androids.AndroidUpgradeDef>
		<defName>Upgrade_ArchotechAIModule</defName>
		<label>Archotech Module</label>
		<description>Replaces the standard AI of the Android with a Archotech one. Considered dangerous by many this can be a boon though. With superior intellectual capabilities everything seem to come natural to the Android with this brain.\n-- "Ascension complete."</description>
		<commandType>Androids.UpgradeCommand_Hediff</commandType>
		<iconTexturePath>Icons/Upgrades/angel-wings</iconTexturePath>
		<orderID>0</orderID>
		<upgradeGroup>Archotech</upgradeGroup>
		<costList>
			<BioMicrochips>1</BioMicrochips>
			<AIPersonaCore>1</AIPersonaCore>
		</costList>
		<costsNotAffectedByBodySize>
			<li>AIPersonaCore</li>
		</costsNotAffectedByBodySize>
		<hediffToApply>ChjAndroidUpgrade_ArchotechAIModule</hediffToApply>
		<extraPrintingTime>150000</extraPrintingTime>
		<requiredResearch>ChJAndroidUpgrade_Archotech</requiredResearch>
	</Androids.AndroidUpgradeDef>
</Defs>