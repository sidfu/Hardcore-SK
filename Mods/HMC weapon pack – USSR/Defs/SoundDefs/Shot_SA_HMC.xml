<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<!-- Groza -->
	<SoundDef>
    <defName>Shot_Groza</defName>
    <context>MapOnly</context>
    <eventNames />
    <maxSimultaneous>1</maxSimultaneous>
	<subSounds>
	<li>
        <grains>
		<li Class="AudioGrain_Clip">
			<clipPath>Groza/Shot_Groza</clipPath>
        </li>
        </grains>
        <volumeRange>
			<min>40</min>
			<max>60</max>
        </volumeRange>
        <pitchRange>
			<min>0.9541177</min>
			<max>1.068824</max>
        </pitchRange>
        <repeatMode>NeverTwice</repeatMode>
        <sustainLoop>False</sustainLoop>
	</li>
	</subSounds>
	</SoundDef>

